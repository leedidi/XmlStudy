<?xml version="1.0" encoding="UTF-8"?>

<!-- 아래 DTD 내용을 토대로 XML 문서를 완성한다. -->
<!-- 데이터는 자신이 좋아하는 가수의 곡 정보를 3곡 이상 등록한다. -->

<!DOCTYPE compactDiscs
[
<!ELEMENT compactDiscs (compactDisc+)>
<!ELEMENT compactDisc (artist, title, tracks, price)>
<!ELEMENT artist (#PCDATA)>
<!ATTLIST artist type (individual | band) #REQUIRED>
<!ELEMENT title (#PCDATA)>
<!ATTLIST title numberOfTracks CDATA #REQUIRED>
<!ELEMENT tracks (track+)>
<!ELEMENT price (#PCDATA)>
<!ELEMENT track (#PCDATA)>
]>
<compactDiscs>
	<compactDisc>
		<artist type="individual">비오</artist>
		<title numberOfTracks="1">Counting Stars</title>
		<tracks></tracks>
		<price></price>
	</compactDisc>
	<compactDisc>
		<artist></artist>
		<title></title>
		<tracks></tracks>
		<price></price>
	</compactDisc>
	<compactDisc>
		<artist></artist>
		<title></title>
		<tracks></tracks>
		<price></price>
	</compactDisc>
</compactDiscs>